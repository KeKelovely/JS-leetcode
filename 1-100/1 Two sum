/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
        for (var i = 0; i < nums.length; i++){
            var j = nums.indexOf(target - nums[i]);
            if ( j !== i && j > -1){
                return [i,j];
            }
        }
};
